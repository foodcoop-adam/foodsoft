- title t('.title'), false

= render 'tabs'

%table.table
  %thead
    %tr
      %th= t '.key'
      %th= t '.value'
  %tbody
    - @keys.each do |key|
      %tr
        %td
          %tt= key
        %td{style: if @cfg[key.to_sym] != @dfl[key.to_sym] then 'font-weight: bold' end}
          = show_config_value key, @cfg[key.to_sym]
